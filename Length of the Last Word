int lengthOfLastWord(char* s) {
     int length = 0;
    int lastWordLength = 0;
    
    while (*s != '\0') {
        if (*s == ' ') {
            if (length != 0) {
                lastWordLength = length;
            }
            length = 0;
        } else {
            length++;
        }
        s++;
    }
    
    if (length != 0) {
        lastWordLength = length;
    }
    
    return lastWordLength;
}
    
